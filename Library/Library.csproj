<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>CreateRandomEntityValues </PackageId>
    <Version>1.0.1</Version>
    <Authors>Rafael Bonaldi - rafaelbonaldi@gmail.com</Authors>
    <Description>Create filled object. Ideal for use in tests and mock.</Description>
    <PackageIcon>logo.png</PackageIcon>
    <PackageReadmeFile>README nuget.md</PackageReadmeFile>
    <PackageProjectUrl>https://github.com/RBonaldi/CreateRandomEntityValues</PackageProjectUrl>
    <RepositoryUrl>https://github.com/RBonaldi/CreateRandomEntityValues</RepositoryUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageTags>CreateRandomEntityValues, CreateRandomEntity, RandomEntity, RandomEntityValues</PackageTags>
    <RepositoryType>GitHub</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Assets\logo.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README nuget.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
